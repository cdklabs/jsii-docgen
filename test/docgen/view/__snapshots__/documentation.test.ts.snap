from aws_cdk import aws_eks

aws_eks.Cluster(scope: Construct, 
                id: str, 
                version: KubernetesVersion, 
                cluster_name: str = None, 
                output_cluster_name: bool = None, 
                output_config_command: bool = None, 
                role: IRole = None, 
                security_group: ISecurityGroup = None, 
