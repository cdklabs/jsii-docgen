def add_lifecycle_rule(description: str = None, 
                       max_image_age: Duration = None, 
                       max_image_count: typing.Union[int, float] = None, 
                       rule_priority: typing.Union[int, float] = None, 
                       tag_prefix_list: typing.List[str] = None, 
