  scope: Construct,
  id: str,
  selectors: typing.List[Selector],
  fargate_profile_name: str = None,
  pod_execution_role: IRole = None,
  subnet_selection: SubnetSelection = None,
  vpc: IVpc = None,
  cluster: Cluster
)
